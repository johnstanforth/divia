{
  "name": "Divia WebParser",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Scrape page-source and post to Divia localserver",
  "browser_action": {
    "default_icon": "diviaparser.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "<all_urls>",
    "http://localhost/*"
  ]
}
